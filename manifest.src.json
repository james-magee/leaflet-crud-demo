{
  "manifest_version": 3,
  "name": "Intramurals Locations",
  "version": "1.0",
  "description": "Shows the intramurals locations",
  "content_scripts": [
    {
      "matches": [
        "https://recreation.ubc.ca/intramurals/leagues/*/term-1-standings/?r=intramurals/*",
        "https://recreation.ubc.ca/intramurals/leagues/*/term-2-standings/?r=intramurals/*"
      ],
      "css": ["content.css"]
    }
  ]
}
